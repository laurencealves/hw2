float grid = 32;
 
 
void setup(){
  background(255, 255, 255);
  size (720, 720);
}
int j=0;
int i=0;
void draw(){
  for ( i = 0 + 30; i < width; i += grid){
    for ( j = 0 + 30; j < height; j += grid){
      noStroke ();
      colorMode (HSB, 360, 100, 100);
      fill (random(360), 100, 100);
         ellipse(i, j, 5, 600);
    }
  }
}
